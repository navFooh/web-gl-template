<!DOCTYPE html>
<html lang="en">

	<head>

		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="description" content="{{ description }}">
		<meta name="keywords" content="{{ keywords }}">
		<meta name="author" content="{{ author }}">
		<meta name="viewport" content="width=device-width, user-scalable=no">

		<meta name="theme-color" content="#000000">
		<meta name="msapplication-tap-highlight" content="no">
		<meta name="msapplication-navbutton-color" content="#000000">
		<meta name="apple-mobile-web-app-capable" content="yes">
		<meta name="apple-mobile-web-app-status-bar-style" content="black">

		<title>{{ title }}</title>

		<link rel="icon" type="image/png" sizes="32x32" href="img/icon-32.png">
		<link rel="icon" type="image/png" sizes="192x192" href="img/icon-192.png">
		<link rel="apple-touch-icon" type="image/png" href="img/icon-192.png">
		<link rel="stylesheet" type="text/css" href="css/style.css?v={{ version }}">

		<meta property="og:title" content="{{ title }}"/>
		<meta property="og:site_name" content="{{ title }}"/>
		<meta property="og:description" content="{{ description }}"/>
		<meta property="og:type" content="website"/>
		<meta property="og:url" content="{{ url }}"/>
		<meta property="og:image" content="{{ url }}/img/og-image.jpg"/>
		<meta property="fb:admins" content="{{ fbAdmin }}"/>

		{{#if dev}}
		<script src="../scripts/vendor/requirejs/require.js" data-main="../scripts/main"></script>
		{{else}}
		<script src="js/main.min.js?v={{ version }}"></script>
		{{/if}}

		{{#unless dev}}
		<script>
			(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
					(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
				m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
			})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
			ga('create', '{{meta.tracking}}', 'auto');
			ga('send', 'pageview');
		</script>
		{{/unless}}

	</head>

	<body data-dev="{{ dev }}"></body>

</html>
